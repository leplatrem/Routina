  },
  "homepage": "https://github.com/leplatrem/Routina#readme",
  "dependencies": {
    "bootstrap": "^3.3.5",
    "btoa": "^1.1.2",
    "moment": "^2.10.6",
    "react": "^0.13.3",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-loader": "^5.3.2",
